{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "$": false
  },
  "rules": {
    "jsx-a11y/href-no-hash": "off",
    "comma-dangle": ["error", "never"],
    "arrow-parens": ["warn", "always"],
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off",
    "jsx-quotes": ["error", "prefer-single"],
    "no-unused-vars": "warn"
  }
}